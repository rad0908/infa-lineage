    <FOLDER NAME="ClaimsFolder" DESCRIPTION="" OWNER="dev" SHARED="NOTSHARED" PERMISSIONS="rwx">
      <MAPPING NAME="m_Analytics" DESCRIPTION="Rename and publish claim metrics to reporting">
        <SOURCE NAME="FACT_CLAIM" DBDNAME="MART" OWNERNAME="DW">
